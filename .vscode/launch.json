{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "app",
      "program": "${workspaceFolder}/vue.config.js"
    },
    {
      "type": "node",
      "request": "launch",
      "preLaunchTask": "tsc",
      "name": "Run Mocha",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "args": [
        "${workspaceRoot}/tests/unit/**/*.js"
      ],
      "cwd": "${workspaceRoot}",
      "outFiles": []
    }
  ]
}
